                    </ns0:Help>
                </ns0:Button>
                
                <ns0:Button caption="Configure" category="ASDST" class="AsdstConfigureButton" id="asdst_addin.cmd_configure" image="Images/GenericWrench_B16.png" message="Configure" tip="Check/set toolbox configuration">
