                    </ns0:Help>
                </ns0:Button>
                
                <ns0:Button caption="Context Calculation" category="ASDST" class="AsdstCalculateContextButton" id="asdst_addin.cmd_calculate_context" image="Images/LocationGetPolygon16.png" message="Make a context calculation" tip="Make a context calculation">
