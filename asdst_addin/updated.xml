            </ns0:Extensions>
            <ns0:Toolbars>
                <ns0:Toolbar caption="ASDST" category="ASDST" id="asdst_addin.toolbar" showInitially="true">
                    <ns0:Items>
                        
                        <ns0:Button refID="asdst_addin.cmd_label" />
                        <ns0:Button refID="asdst_addin.cmd_create_project" />
                        
                        <ns0:Button refID="asdst_addin.cmd_calculate_context" />
                        <ns0:Button refID="asdst_addin.cmd_configure" />
