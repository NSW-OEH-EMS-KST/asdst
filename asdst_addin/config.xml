<ESRI.Configuration xmlns="http://schemas.esri.com/Desktop/AddIns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Name>ASDST
    </Name>
    <AddInID>{890damf9-af62-4020-badf-a72af1268a61}
    </AddInID>
    <Description>Aboriginal Site Decision Support Tools
    </Description>
    <Version>0.2
    </Version><Image />
    <Author>D.Bye, KST, EMS, NSW OEH
    </Author>
    <Company>NSW OEH
    </Company>
    <Date>2018-01-09 14:28
    </Date>
    <Targets>
        <Target name="Desktop" version="10.4" />
    </Targets>
    <AddIn language="PYTHON" library="asdst_addin.py" namespace="asdst_addin">
        <ArcMap>
            <Commands>
                <Button caption="Create New ASDST Project"
                        category="ASDST"
                        class="CreateProjectButton"
                        id="asdst_addin.cmd_create_project"
                        image="Images/AddInDesktop16.png"
                        message="Create an empty ASDST project"
                        tip="Create Project">
                    <Help heading="ASDST help ?">TO DO
                    </Help>
                </Button>
                <Button caption="Build Data"
                        category="ASDST"
                        class="BuildDataButton"
                        id="asdst_addin.cmd_build_data"
                        image="Images/GeoprocessingPackage16.png"
                        message="Build project data"
                        tip="Build Current, Pre-1750 and Loss Data">
                    <Help heading="ASDST help ?">TO DO
                    </Help>
                </Button>
                <Button caption="Calculate Context"
                        category="ASDST"
                        class="CalculateContextButton"
                        id="asdst_addin.cmd_calculate_context"
                        image="Images/LocationGetPolygon16.png"
                        message="Calculate the heritage context"
                        tip="Calculates the heritage context">
                    <Help heading="ASDST help ?">TO DO
                    </Help>
                </Button>
                <Button caption="Configure"
                        category="ASDST"
                        class="ConfigureButton"
                        id="asdst_addin.cmd_configure"
                        image="Images/GenericWrench_B16.png"
                        message="Configure"
                        tip="Check/set toolbox configuration">
                    <Help heading="ASDST help ?">TO DO
                    </Help>
                </Button>
                <Button caption="ASDST"
                        category="ASDST"
                        class="InfoButton"
                        id="asdst_addin.cmd_info"
                        image=""
                        message="Display ASDST status"
                        tip="Reflects the current ASDST configuration and project status">
                    <Help heading="ASDST help ?">TO DO
                    </Help>
                </Button>
            </Commands>
            <Extensions>
                <Extension autoLoad="True" category="ASDST"
                           class="AsdstExtension"
                           description="Aboriginal Sites Decision Support Tools" id="asdst_addin.extension"
                           name="ASDST" productName="ASDST" showInExtensionDialog="True" />
            </Extensions>
            <Toolbars>
                <Toolbar caption="ASDST" category="ASDST" id="asdst_addin.toolbar"  showInitially="true">
                    <Items>
                        <Button refID="asdst_addin.cmd_info" />
                        <Button refID="asdst_addin.cmd_configure" />
                        <Button refID="asdst_addin.cmd_create_project" />
                        <Button refID="asdst_addin.cmd_build_data" />
                        <Button refID="asdst_addin.cmd_calculate_context" />
                    </Items>
                </Toolbar>
            </Toolbars>
            <Menus>
            </Menus>
        </ArcMap>
    </AddIn>
</ESRI.Configuration>