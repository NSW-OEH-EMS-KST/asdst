    <ns0:AddIn language="PYTHON" library="asdst_addin.py" namespace="asdst_addin">
        <ns0:ArcMap>
            <ns0:Commands>
                <ns0:Button caption="Create New ASDST Project" category="ASDST" class="AsdstCreateProjectButton" id="asdst_addin.cmd_create_project" image="Images/AddInDesktop16.png" message="This tool creates an empty ASDST project which is the first step in an ASDST analysis workflow" tip="Create Project">
